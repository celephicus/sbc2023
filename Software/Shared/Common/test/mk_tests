#!/usr/bin/env bash

make BUILD_DIR=b_16_32 clean
make BUILD_DIR=b_16_32 \
EXTRAS='-DCFG_MYPRINTF_TYPE_UNSIGNED_INT=uint16_t -DCFG_MYPRINTF_TYPE_SIGNED_INT=int16_t \
-DCFG_MYPRINTF_TYPE_UNSIGNED_LONG_INT=uint32_t -DCFG_MYPRINTF_TYPE_SIGNED_LONG_INT=int32_t'

make BUILD_DIR=b_32_64 clean
make BUILD_DIR=b_32_64 \
EXTRAS='-DCFG_MYPRINTF_TYPE_UNSIGNED_INT=uint32_t -DCFG_MYPRINTF_TYPE_SIGNED_INT=int32_t \
-DCFG_MYPRINTF_TYPE_UNSIGNED_LONG_INT=uint64_t -DCFG_MYPRINTF_TYPE_SIGNED_LONG_INT=int64_t'

make BUILD_DIR=b_64_128 clean
make BUILD_DIR=b_64_128 \
EXTRAS='-DCFG_MYPRINTF_TYPE_UNSIGNED_INT=uint64_t -DCFG_MYPRINTF_TYPE_SIGNED_INT=int64_t \
-DCFG_MYPRINTF_TYPE_UNSIGNED_LONG_INT="unsigned __int128" -DCFG_MYPRINTF_TYPE_SIGNED_LONG_INT=__int128'



